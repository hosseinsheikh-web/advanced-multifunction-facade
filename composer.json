    "license": "MIT",
    "keywords": ["laravel", "laravel9", "laravel8-package", "laravel9-package", "laravel-package", "advanced-multifunction-facade"],
    "license": "MIT",
    "homepage": "https://github.com/hosseinsheikh-web/advanced-multifunction-facade",
    "authors": [
